        "bodyPatterns": [
            {
                "equalToJson": {
                    "externalId": "schema-1",
                    "name": "Test schema 1",
                    "fields": [
                        {
                            "name": "stringField",
                            "dataType": "STRING",
                            "comments": null
                        },
                        {
                            "name": "numericField",
